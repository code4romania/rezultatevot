{
    "name": "code4romania/rezultatevot",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "filament"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "andreiio/blade-remix-icon": "^3.6",
        "archtechx/laravel-seo": "^0.10",
        "awcodes/filament-tiptap-editor": "^3.5",
        "blade-ui-kit/blade-icons": "^1.8",
        "codeat3/blade-google-material-design-icons": "^1.20",
        "datlechin/filament-menu-builder": "^0.7",
        "dedoc/scramble": "^0.12",
        "filament/filament": "^3.3",
        "filament/spatie-laravel-media-library-plugin": "^3.3",
        "haydenpierce/class-finder": "^0.5",
        "jeffgreco13/filament-breezy": "^2.6",
        "laravel/framework": "^12.19",
        "laravel/horizon": "^5.32",
        "laravel/sanctum": "^4.1",
        "laravel/scout": "^10.15",
        "laravel/tinker": "^2.10",
        "league/csv": "^9.23",
        "league/flysystem-aws-s3-v3": "^3.29",
        "league/flysystem-read-only": "^3.28",
        "livewire/livewire": "^3.6",
        "maatwebsite/excel": "^3.1",
        "sentry/sentry-laravel": "^4.13",
        "spatie/temporary-directory": "^2.3",
        "staudenmeir/belongs-to-through": "^2.16",
        "tpetry/laravel-query-expressions": "^1.5",
        "typesense/typesense-php": "^5.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.15",
        "barryvdh/laravel-ide-helper": "^3.5",
        "fakerphp/faker": "^1.24",
        "friendsofphp/php-cs-fixer": "^3.75",
        "larastan/larastan": "^3.4",
        "laravel/pint": "^1.22",
        "laravel/sail": "^1.43",
        "laravel/telescope": "^5.8",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.7",
        "phpunit/phpunit": "^11.5",
        "spatie/laravel-ignition": "^2.9"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan filament:upgrade",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models --nowrite"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "test": [
            "@php artisan test"
        ],
        "analyse": [
            "./vendor/bin/phpstan analyse"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
